{
    "name": "ohseesoftware/laravel-markdown-content",
    "description": "Laravel Markdown Content is an opinionated package that aims to make adding markdown generated pages to your site a breeze.",
    "keywords": [
        "ohseesoftware",
        "laravel"
    ],
    "homepage": "https://github.com/ohseesoftware/laravel-markdown-content",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Owen Conti",
            "email": "owen@ohseemedia.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.80",
        "ryangjchandler/orbit": "^0.9"
    },
    "require-dev": {
        "orchestra/testbench": "^6.7",
        "php-coveralls/php-coveralls": "^2.4",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "OhSeeSoftware\\LaravelMarkdownContent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OhSeeSoftware\\LaravelMarkdownContent\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "OhSeeSoftware\\LaravelMarkdownContent\\LaravelMarkdownContentServiceProvider"
            ]
        }
    }
}
